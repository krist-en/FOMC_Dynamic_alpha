"\n","    # Drop rows with NaN values\n","    X = X.dropna()\n","    y = y.loc[X.index]\n","\n","    # Add a constant term for the intercept\n","    X = sm.add_constant(X)\n","\n","    # Fit the model\n","    model = sm.OLS(y, X).fit()\n","\n","    # Store the results\n","    results[dep_var] = model\n","\n","# Display the summary for each model\n","for dep_var, model in results.items():\n","    print(f\"Summary for {dep_var}:\\n\")\n","    print(model.summary())\n","    print(\"\\n\" + \"=\"*80 + \"\\n\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"pmpjqvBMNnHo","executionInfo":{"status":"ok","timestamp":1719782279190,"user_tz":240,"elapsed":582,"user":{"displayName":"zhihao wang","userId":"04684194927419039210"}},"outputId":"ba0eb6fc-40cf-42d0-8972-819a3f371250"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Summary for S5AEROX _rf:\n","\n","                            OLS Regression Results                            \n","==============================================================================\n","Dep. Variable:            S5AEROX _rf   R-squared:                       0.703\n","Model:                            OLS   Adj. R-squared:                  0.615\n","Method:                 Least Squares   F-statistic:                     7.997\n","Date:                Sun, 30 Jun 2024   Prob (F-statistic):           1.77e-05\n","Time:                        21:17:57   Log-Likelihood:                 32.827\n","No. Observations:                  36   AIC:                            -47.65\n","Df Residuals:                      27   BIC:                            -33.40\n","Df Model:                           8                                         \n","Covariance Type:            nonrobust                                         \n","==================================================================================\n","                     coef    std err          t      P>|t|      [0.025      0.975]