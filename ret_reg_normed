model = sm.OLS(y, X).fit()\n","\n","        # Store the results\n","        results[dep_var] = model\n","    except KeyError as e:\n","        print(f\"Error with dependent variable {dep_var}: {e}\")\n","\n","# Display the summary for each model\n","for dep_var, model in results.items():\n","    print(f\"Summary for {dep_var}:\\n\")\n","    print(model.summary())\n","    print(\"\\n\" + \"=\"*80 + \"\\n\")\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"_kOROr0zQTqG","executionInfo":{"status":"ok","timestamp":1719781548378,"user_tz":240,"elapsed":586,"user":{"displayName":"zhihao wang","userId":"04684194927419039210"}},"outputId":"0b1fcffd-de5d-4445-a136-6b6a914f3ce6"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Dependent Variables: Index(['S5AEROX _rf', 'S5BANKX_rf', 'S5TECH_rf', 'S5FDBT_rf', 'S5INSU_rf',\n","       'S5TRAN_rf', 'S5TELS_rf', 'S5REITS_rf', 'SPSIBI_rf', 'SPSIHO_rf',\n","       'SPSIMM_rf', 'SPSIOP_rf', 'SPSIPH_rf', 'SPSIRE_rf', 'SPSISC_rf',\n","       'S5UTIL_rf', 'S5MATR_rf', 'S5FINL_rf'],\n","      dtype='object')\n","Corresponding 30d Variables: Index(['S5AEROX _rf_3d', 'S5BANKX_rf_3d', 'S5TECH_rf_3d', 'S5FDBT_rf_3d',\n","       'S5INSU_rf_3d', 'S5TRAN_rf_3d', 'S5TELS_rf_3d', 'S5REITS_rf_3d',\n","       'SPSIBI_rf_3d', 'SPSIHO_rf_3d', 'SPSIMM_rf_3d', 'SPSIOP_rf_3d',\n","       'SPSIPH_rf_3d', 'SPSIRE_rf_3d', 'SPSISC_rf_3d', 'S5UTIL_rf_3d',\n","       'S5MATR_rf_3d', 'S5FINL_rf_3d'],\n","      dtype='object')\n","Other Independent Variables: ['EFFR_CHG', 'MPU_wrld', 'NE', 'macro', 'Chair_2', 'Chair_3']\n","Date Variables: []\n","Summary for S5AEROX _rf:\n","\n","                            OLS Regression Results                            \n","==============================================================================\n","Dep. Variable:            S5AEROX _rf   R-squared:                       0.141\n","Model:                            OLS   Adj. R-squared:                  0.009\