erce')\n","        y = pd.to_numeric(y, errors='coerce')\n","\n","        # Drop rows with NaN values\n","        X = X.dropna()\n","        y = y.loc[X.index]\n","\n","        # Add a constant term for the intercept\n","        X = sm.add_constant(X)\n","\n","        # Fit the model\n","        model = sm.OLS(y, X).fit()\n","\n","        # Store the results\n","        results[dep_var] = model\n","    except KeyError as e:\n","        print(f\"Error with dependent variable {dep_var}: {e}\")\n","\n","# Display the summary for each model\n","for dep_var, model in results.items():\n","    print(f\"Summary for {dep_var}:\\n\")\n","    print(model.summary())\n","    print(\"\\n\" + \"=\"*80 + \"\\n\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"fxMVVmpvhZLr","executionInfo":{"status":"ok","timestamp":1719779015517,"user_tz":240,"elapsed":1438,"user":{"displayName":"zhihao wang","userId":"04684194927419039210"}},"outputId":"b1b41187-c530-4763-f774-cae90683da11"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Dependent Variables: Index(['S5AEROX _rf', 'S5BANKX_rf', 'S5TECH_rf', 'S5FDBT_rf', 'S5INSU_rf',\n","       'S5TRAN_rf', 'S5TELS_rf', 'S5REITS_rf', 'SPSIBI_rf', 'SPSIHO_rf',\n","       'SPSIMM_rf', 'SPSIOP_rf', 'SPSIPH_rf', 'SPSIRE_rf', 'SPSISC_rf',\n","       'S5UTIL_rf', 'S5MATR_rf', 'S5FINL_rf'],\n","      dtype='object')\n","Corresponding 30d Variables: Index(['S5AEROX _rf_3d', 'S5BANKX_rf_3d', 'S5TECH_rf_3d', 'S5FDBT_rf_3d',\n","       'S5INSU_rf_3d', 'S5TRAN_rf_3d', 'S5TELS_rf_3d', 'S5REITS_rf_3d',\n","       'SPSIBI_rf_3d', 'SPSIHO_rf_3d', 'SPSIMM_rf_3d', 'SPSIOP_rf_3d',\n","       'SPSIPH_rf_3d', 'SPSIRE_rf_3d', 'SPSISC_rf_3d', 'S5UTIL_rf_3d',\n","       'S5MATR_rf_3d', 'S5FINL_rf_3d'],\n","      dtype='object')\n","Other Independent Variables: ['Negative Emotions Avg', 'EFFR_CHG', 'MPU_wrld', 'macro', 'Chair_2', 'Chair_3', 'Quarter_2', 'Quarter_3', 'Quarter_4']\n","Date Variables: []\n","Summary for